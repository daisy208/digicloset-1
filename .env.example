### Environment variables for DigiCloset (example)
# FastAPI / app
APP_ENV=production
LOG_LEVEL=INFO

# Redis
REDIS_URL=redis://redis:6379/0

# AI settings
AI_MODEL_NAME=gpt2
AI_INFERENCE_TIMEOUT=5
AI_CB_FAILURE_THRESHOLD=3
AI_CB_RESET_TIMEOUT=30

# Shopify
SHOPIFY_API_KEY=
SHOPIFY_API_SECRET=

# CORS
ALLOWED_ORIGINS=https://your-shop-domain.com
